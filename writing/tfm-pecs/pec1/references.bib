@article{jonesLongreadRNASequencing2024,
  title = {Long-Read {{RNA}} Sequencing Identifies Region- and Sex-Specific {{C57BL}}/{{6J}} Mouse Brain {{mRNA}} Isoform Expression and Usage},
  author = {Jones, Emma F. and Howton, Timothy C. and Flanary, Victoria L. and Clark, Amanda D. and Lasseigne, Brittany N.},
