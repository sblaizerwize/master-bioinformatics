@article{innesGeneExpressionAlternative2024,
  title = {Gene Expression and Alternative Splicing Contribute to Adaptive Divergence of Ecotypes},
  author = {Innes, Peter A. and Goebl, April M. and Smith, Chris C. R. and Rosenberger, Kaylee and Kane, Nolan C.},
  year = 2024,
  month = mar,
  journal = {Heredity},
  volume = {132},
  number = {3},
  pages = {120--132},
